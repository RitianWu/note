{
  "description": "程序员笔记",
  "plugins": [
    "sitemap",
    "disqus",
    "github",
    "github-buttons",
    "simple-page-toc",
    "edit-link"
  ],
  "pluginsConfig": {
    "sitemap": {
      "hostname": "https://note.grianchan.com/"
    },
    "disqus": {
      "shortName": "gitbookuse"
    },
    "github": {
      "url": "https://github.com/zhan3333"
    },
    "github-buttons": {
      "repo": "zhan3333/note",
      "types": [
        "star",
        "watch",
        "fork"
      ]
    },
    "edit-link": {
      "base": "https://github.com/zhan3333/note",
      "label": "编辑本页"
    },
    "size": "small",
    "simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": true
    }
  }
}